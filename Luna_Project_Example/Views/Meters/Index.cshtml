@model IEnumerable<Luna_Project_Example.Models.Meter>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.meterID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.valveWidth)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.numberOfSubs)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.maxNumberOfSubs)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.productionDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.batteryState)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.meterID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.valveWidth)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.numberOfSubs)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.maxNumberOfSubs)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.productionDate)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.batteryState)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.meterID }) |
        @Html.ActionLink("Details", "Details", new { id = item.meterID }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.meterID }) |
        @Html.ActionLink("Associate", "Associate", new { id = item.meterID })
    </td>
</tr>
}

</table>
